{
  "name": "panels-ui",
  "version": "0.1.1",
  "description": "Panels UI components",
  "main": "index.js",
  "scripts": {
    "test": ".bin/test",
    "prepublish": ".bin/compile",
    "postpublish": ".bin/clean",
    "start": ".bin/start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UXtemple/panels-ui.git"
  },
  "keywords": [
    "panels",
    "ui",
    "components",
    "UXtemple"
  ],
  "author": "UXtemple Limited <hi@uxtemple.com>",
  "contributors": [
    "Dar√≠o Javier Cravero <dario@uxtemple.com>",
    "Tom Parandyk <tom@uxtemple.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UXtemple/panels-ui/issues"
  },
  "homepage": "https://github.com/UXtemple/panels-ui",
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "defaultParams": true,
      "forOf": true,
      "jsx": true,
      "templateStrings": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "eqeqeq": 2,
      "curly": 2,
      "new-cap": false,
      "quotes": [
        2,
        "single"
      ],
      "strict": 0
    }
  },
  "browserify": {
    "extensions": [
      ".es6"
    ],
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babelify": "^6.0.2",
    "browserify": "^10.1.3",
    "envify": "^3.4.0",
    "livereloadify": "^2.0.0",
    "node-static": "^0.7.6",
    "uglify-js": "^2.4.21",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "immutable": "^3.7.2",
    "react": "^0.13.2"
  }
}
