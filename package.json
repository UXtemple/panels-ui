{
  "name": "panels-ui",
  "description": "Main panels UI components",
  "jsnext:main": "dist/next.js",
  "main": "dist/cjs.js",
  "scripts": {
    "bundle": "BABEL_ENV=cjs rollup -c .rollup/cjs.js && BABEL_ENV=es rollup -c .rollup/next.js",
    "prepublish": "npm run bundle",
    "postpublish": "rm -rf dist && git checkout package.json && git push --tags",
    "push": "git tag $(npm version $VERSION) && npm publish",
    "test": ".bin/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UXtemple/panels-ui.git"
  },
  "keywords": [
    "panels",
    "ui",
    "components",
    "UXtemple"
  ],
  "author": "UXtemple Limited <hi@uxtemple.com>",
  "contributors": [
    "Dar√≠o Javier Cravero <dario@uxtemple.com>",
    "Tom Parandyk <tom@uxtemple.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UXtemple/panels-ui/issues"
  },
  "homepage": "https://github.com/UXtemple/panels-ui",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-plugin-external-helpers": "^6.4.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.4.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.4.0",
    "babel-plugin-transform-es2015-classes": "^6.4.5",
